	<row id="pl_msg_00000217_00111" index="111">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hi! This is the registration counter\nfor practice sessions.\rYou may practice in the categories\nof Visual, Dance, and Acting.\r</language>
	</row>
	<row id="pl_msg_00000217_00112" index="112">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Would you like to join a practice\nsession?</language>
	</row>
