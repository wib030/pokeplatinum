	<row id="pl_msg_00000635_00092" index="92">
		<attribute name="window_context_name">used</attribute>
		<language name="English">There is an Egg that cannot be traded\nin your party.</language>
	</row>
	<row id="pl_msg_00000635_00093" index="93">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Spin Trades are so much fun!</language>
	</row>
	<row id="pl_msg_00000635_00094" index="94">
		<attribute name="window_context_name">used</attribute>
		<language name="English">There!\nStop there!</language>
	</row>
	<row id="pl_msg_00000635_00095" index="95">
		<attribute name="window_context_name">used</attribute>
		<language name="English">My Egg’s awesome!</language>
	</row>
	<row id="pl_msg_00000635_00096" index="96">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Yes!\nI got the Egg I wanted!</language>
	</row>
	<row id="pl_msg_00000635_00097" index="97">
		<attribute name="window_context_name">used</attribute>
		<language name="English">{STRVAR_1 28, 2, 0}{PAUSE 15}\rHello, it’s {STRVAR_1 3, 0, 0}!\nWould you like to do something?</language>
	</row>
	<row id="pl_msg_00000635_00098" index="98">
		<attribute name="window_context_name">used</attribute>
