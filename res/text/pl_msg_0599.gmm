	<row id="pl_msg_00000599_00013" index="13">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Oh, please, {STRVAR_1 3, 0, 0}.\rYou shouldn’t joke about not\nhaving money.</language>
	</row>
	<row id="pl_msg_00000599_00014" index="14">
		<attribute name="window_context_name">used</attribute>
		<language name="English">I see.\nPlease do visit again.</language>
